#include <stdio.h>
#include <math.h>

int main() {
  int a, min, max, suma, i;
  printf("Wprowadź 3 liczby całkowite : \n");
  scanf("%i",&a);
  suma = a;
  min = a;
  max = a;
  
  for( i = 0; i < 2; i++) {
    scanf("%i",&a);
    if(a < min) min = a;
    if(a > max) max = a;
    suma += a;
  }
  return 0;
}
